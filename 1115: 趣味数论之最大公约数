while 1:
    num, num1 = map(int, input().split())
    if num == num1 == 0:
        break
    mi = min(num, num1)
    ma = max(num, num1)
    if ma % mi == 0:
        print(mi)
        continue
    c = 0
    for a in range(1, mi//2+1):
        if mi % a == 0 and ma % a == 0:
            c = a
    print(c)
